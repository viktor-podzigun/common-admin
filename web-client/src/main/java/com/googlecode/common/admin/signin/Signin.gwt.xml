<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "http://google-web-toolkit.googlecode.com/svn/releases/2.4/distro-source/core/src/gwt-module.dtd">
<module rename-to='signin'>
    
    <!-- App modules -->
    <inherits name='com.googlecode.common.App' />
    <inherits name='com.googlecode.common.admin.protocol.Protocol' />
    
    
    <!-- Specify the app entry point class -->
    <entry-point class='com.googlecode.common.admin.signin.client.SigninClient' />
    
    <!-- Specify the paths for translatable code -->
    <source path="client" />

</module>
